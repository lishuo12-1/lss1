<!--pages/gird/gird.wxml-->
<view class="page">
    <view class="page__hd">
        <view class="page__title">Grid</view>
        <view class="page__desc">九宫格</view>
    </view>
    <view class="page__bd">
        <view class="weui-grids">
            <block wx:for="{{grids}}" wx:key="*this">
                <navigator url="http://www.baidu.com" class="weui-grid" hover-class="weui-grid_active">
                    <image class="weui-grid__icon" src="../images/1.gif" />
                    <view class="weui-grid__label">{{item}}</view>
                </navigator>
            </block>
        </view>
    </view>
</view>
